{
  "name": "front-data",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:budget": "ng serve --project=budget-dataetat --configuration development",
    "start:budget-dev": "ng serve --project=budget-dataetat --proxy-config proxy.conf.json --port=4210",
    "start:france-relance": "ng serve --project=france-relance",
    "start:france-relance-dev": "ng serve --project=france-relance --proxy-config proxy.conf.json --port=5006",
    "start:data-qpv": "ng serve --project=data-qpv",
    "start:data-qpv-dev": "ng serve --project=data-qpv --proxy-config proxy.conf.json",
    "build:budget": "ng build --project=budget-dataetat",
    "build:budget-dev": "ng build --project=budget-dataetat --output-path=dist/budget-dataetat-dev/",
    "build:france-relance": "ng build --project=france-relance",
    "build:data-qpv": "ng build --project=data-qpv",
    "build:all": "ng build --project=france-relance && ng build --project=financial-data && ng build --project=data-qpv",
    "storybook:common-lib": "ng run common-lib:storybook",
    "test-storybook": "test-storybook --config-dir=apps/common-lib/.storybook",
    "lint": "eslint --max-warnings 55"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.0",
    "@angular/cdk": "^20.0.3",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/material": "^20.0.3",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@edugouvfr/ngx-dsfr-ext": "^1.4.0",
    "@gouvfr/dsfr": "^1.14.0",
    "chart.js": "4.5.1",
    "geoportal-extensions-openlayers": "^3.4.4",
    "keycloak-angular": "^20.0.0",
    "keycloak-js": "^25.0.6",
    "ngx-matomo-client": "^8.0.0",
    "ol": "^10.2.1",
    "ol-mapbox-style": "^12.3.5",
    "rxjs": "~7.8.0",
    "topojson-client": "^3.1.0",
    "tslib": "^2.3.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.1/xlsx-0.20.1.tgz",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.9",
    "@angular-eslint/builder": "20.5.2",
    "@angular-eslint/eslint-plugin": "20.5.2",
    "@angular-eslint/eslint-plugin-template": "20.5.2",
    "@angular-eslint/schematics": "20.5.2",
    "@angular-eslint/template-parser": "20.5.2",
    "@angular/build": "^20.0.3",
    "@angular/cli": "^20.0.3",
    "@angular/compiler-cli": "^21.0.0",
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-docs": "^10.0.0",
    "@storybook/addon-links": "^10.0.0",
    "@storybook/angular": "^10.0.0",
    "@storybook/test-runner": "^0.24.0",
    "@types/arcgis-rest-api": "^10.4.8",
    "@types/jasmine": "~5.1.0",
    "@types/topojson-specification": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.17.0",
    "eslint-plugin-storybook": "^10.0.0",
    "globals": "^16.0.0",
    "ng-packagr": "^20.0.1",
    "storybook": "^10.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.28.0"
  },
  "overrides": {
    "browser-sync": "^3.0.2"
  }
}
