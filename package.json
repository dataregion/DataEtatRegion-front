{
  "name": "front-data",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:financial": "ng serve --project=financial-data",
    "start:financial-dev": "ng serve --project=financial-data --proxy-config proxy.conf.json",
    "start:france-relance": "ng serve --project=france-relance",
    "start:france-relance-dev": "ng serve --project=france-relance --proxy-config proxy.conf.json --port=5006",
    "start:data-qpv": "ng serve --project=data-qpv",
    "start:data-qpv-dev": "ng serve --project=data-qpv --proxy-config proxy.conf.json",
    "build:financial-data": "ng build --project=financial-data",
    "build:france-relance": "ng build --project=france-relance",
    "build:data-qpv": "ng build --project=data-qpv",
    "build:all": "ng build --project=france-relance && ng build --project=financial-data && ng build --project=data-qpv",
    "storybook:common-lib": "ng run common-lib:storybook",
    "test-storybook": "test-storybook --config-dir=apps/common-lib/.storybook",
    "lint": "ng lint"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.5",
    "@angular/cdk": "^19.0.4",
    "@angular/common": "^19.0.5",
    "@angular/compiler": "^19.0.5",
    "@angular/core": "^19.0.5",
    "@angular/forms": "^19.0.5",
    "@angular/material": "^19.0.4",
    "@angular/platform-browser": "^19.0.5",
    "@angular/platform-browser-dynamic": "^19.0.5",
    "@angular/router": "^19.0.5",
    "@edugouvfr/ngx-dsfr": "^1.12.4",
    "@gouvfr/dsfr": "^1.12.1",
    "geoportal-extensions-openlayers": "^3.4.4",
    "keycloak-angular": "^19.0.2",
    "keycloak-js": "^25.0.6",
    "ngx-logger": "^5.0.12",
    "ngx-matomo-client": "^7.0.0",
    "ol": "^10.2.1",
    "ol-mapbox-style": "^12.3.5",
    "rxjs": "~7.8.0",
    "topojson-client": "^3.1.0",
    "tslib": "^2.3.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.1/xlsx-0.20.1.tgz",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular-eslint/builder": "19.0.2",
    "@angular-eslint/eslint-plugin": "19.0.2",
    "@angular-eslint/eslint-plugin-template": "19.0.2",
    "@angular-eslint/schematics": "19.0.2",
    "@angular-eslint/template-parser": "19.0.2",
    "@angular/cli": "~19.1.0",
    "@angular/compiler-cli": "^19.0.5",
    "@chromatic-com/storybook": "^3.2.3",
    "@playwright/test": "^1.44.1",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/angular": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@storybook/test-runner": "^0.21.0",
    "@types/arcgis-rest-api": "^10.4.8",
    "@types/jasmine": "~4.6.0",
    "@types/topojson-specification": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.17.0",
    "eslint-plugin-storybook": "^0.11.2",
    "ng-packagr": "^19.0.1",
    "storybook": "^8.4.7",
    "typescript": "^5.5.4"
  },
  "_comment": "TODO: Le temps que ngx-matomo-client ai une release compatible avec angular 19",
  "overrides": {
    "browser-sync": "^3.0.2",
    "ngx-matomo-client": {
      "@angular/common@": ">=18.0.0",
      "@angular/core@": ">=18.0.0"
    }
  }
}
